{
    "private": true,
    "name": "t200logs",
    "displayName": "T21 Logs Viewer",
    "description": "Extension to make diagnosing Teams logs much easier. Open this extension in a folder containing Teams logs and it will combine them into a single view with filtering and visual hints.",
    "repository": {
        "type": "git",
        "url": "https://github.com/felixSchober/t200Logs"
    },
    "license": "MIT",
    "version": "0.0.3",
    "author": {
        "name": "Felix Schober",
        "email": "github@felix-schober.de"
    },
    "preview": true,
    "publisher": "FelixSchober",
    "engines": {
        "vscode": "^1.84.0"
    },
    "icon": "media/ext_icon.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onFileSystem:scheme:**/desktop/**"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "t200logs.openLog",
                "title": "Opens the extension log file",
                "category": "T200Logs",
                "shortTitle": "Show Logs"
            },
            {
                "command": "t200logs.provideFeedback",
                "title": "Provide quick feedback",
                "category": "T200Logs",
                "shortTitle": "Feedback"
            },
            {
                "command": "t200logs.toggleReadableIsoDates",
                "title": "Toggles readable dates to log viewer",
                "category": "T200Logs",
                "shortTitle": "Toggle readable dates"
            },
            {
                "command": "t200logs.openLogViewer",
                "title": "Open log viewer",
                "category": "T200Logs",
                "shortTitle": "Open Log Viewer",
                "icon": "media/icon_filled.png"
            },
            {
                "command": "t200logs.reset",
                "title": "Reset all data and reopen log viewer",
                "category": "T200Logs",
                "shortTitle": "Reset Log Viewer",
                "icon": "media/reset.png"
            },
            {
                "command": "t200logs.toggleVisualHints",
                "title": "Toggles visual hints in log viewer",
                "category": "T200Logs",
                "shortTitle": "Toggle visual hints"
            },
            {
                "command": "t200logs.inlineDateFilter",
                "title": "Inline date filter",
                "category": "T200Logs"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "t200logs",
                    "title": "T200Logs",
                    "icon": "media/icon_outline.png"
                }
            ]
        },
        "views": {
            "t200logs": [
                {
                    "type": "webview",
                    "id": "t200logs",
                    "name": "Teams Logs Viewer"
                }
            ]
        },
        "menus": {
            "editor/title": [
                {
                    "command": "t200logs.reset",
                    "group": "navigation",
                    "when": "resourceScheme == log-viewer"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "vsce-package": "mkdirp out && vsce package -o out/t200logs.vsix --yarn",
        "vsce-package-local": "mkdirp out && vsce package -o out/t200logs.vsix --yarn",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "installWithPublicRegistry": "install --registry=https://registry.yarnpkg.com"
    },
    "dependencies": {
        "@vscode/extension-telemetry": "^0.9.2"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.3",
        "@types/node": "18.x",
        "@types/vscode": "^1.84.0",
        "@typescript-eslint/eslint-plugin": "^7.0.0-alpha.0",
        "@typescript-eslint/parser": "^7.0.0-alpha.0",
        "@vscode/test-electron": "^2.3.6",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jsdoc": "^46.9.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-security": "^1.7.1",
        "glob": "^10.3.10",
        "mocha": "^10.2.0",
        "prettier": "^3.1.0",
        "prettier-package-json": "^2.8.0",
        "ts-loader": "^9.5.0",
        "ts-prune": "^0.10.3",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    }
}
