{
    "categories": [
    {
        "title": "## 🚀 Features",
        "labels": [
            "type: feature request",
            "type: enhancement",
            "type: epic"
        ],
        "empty_content": "- no matching PRs"
    },
    {
        "title": "## 🐛 Bug Fixes",
        "labels": [
            "type: bug"
        ],
        "empty_content": "- no matching PRs"
    },
    {
        "title": "## 🧪 Maintenance",
        "labels": [
            "dependencies",
            "type: chore",
            "type: ci",
            "type: dependencies",
            "documentation"
        ],
        "empty_content": "- no matching PRs"
    }
    ],
    "ignore_labels": [
    "duplicate",
    "question",
    "invalid",
    "wontfix"
    ],
    "template": "#{{CHANGELOG}}",
    "pr_template": "- #{{TITLE}}",
    "label_extractor": [
        {
            "pattern": "^(build|bump|BUMP|Bump|bumps|chore|ci|docs|feat|fix|perf|refactor|revert|style|test){1}(\\([\\w\\-\\.]+\\))?(!)?: ([\\w ])+([\\s\\S]*)",
            "target": "$1"
        }
    ],
    "base_branches": [
        "dev"
    ]
}
  