{
    "categories": [
    {
        "title": "üöÄ Features",
        "labels": [
            "type: feature request",
            "type: enhancement",
            "type: epic"
        ]
    },
    {
        "title": "üêõ Bug Fixes",
        "labels": [
            "type: bug"
        ]
    },
    {
        "title": "üß™ Maintenance",
        "labels": [
            "dependencies",
            "type: chore",
            "type: ci",
            "type: dependencies",
            "documentation"
        ]
    }
    ],
    "ignore_labels": [
    "duplicate",
    "question",
    "invalid",
    "wontfix"
    ],
    "change_template": "- $TITLE @$AUTHOR (#$NUMBER)",
    "no_changes_template": "No changes in this category.",
    "version_template": "$RESOLVED_VERSION",
    "configuration": {
        "issue_resolver": {
            "pattern": "((?:close|closes|closed|fix|fixes|fixed|resolve|resolves|resolved) #([0-9]+))",
            "url": "https://github.com/felixSchober/t200Logs/issues/$NUMBER"
        },
    "pr_resolver": {
        "pattern": "#([0-9]+)",
        "url": "https://github.com/felixSchober/t200Logs/pulls/$NUMBER"
    }
    }
}
  