{
    "categories": [
    {
        "title": "#### 🚀 Features",
        "labels": [
            "type: feature request",
            "type: enhancement",
            "type: epic",
            "feat:"
        ]
    },
    {
        "title": "#### 🐛 Bug Fixes",
        "labels": [
            "type: bug",
            "fix:"
        ]
    },
    {
        "title": "#### 🧪 Maintenance",
        "labels": [
            "dependencies",
            "type: chore",
            "type: ci",
            "type: dependencies",
            "documentation",
            "bump"
        ]
    }
    ],
    "ignore_labels": [
        "duplicate",
        "question",
        "invalid",
        "wontfix"
    ],
    "template": "#{{CHANGELOG}}\n\n#### 📦 Uncategorized\n\n${{UNCATEGORIZED}}",
    "pr_template": "- #{{TITLE}}",
    "base_branches": [
        "dev"
    ]
}
  