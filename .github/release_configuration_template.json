{
    "categories": [
    {
        "title": "## 🚀 Features",
        "labels": [
            "type: feature request",
            "type: enhancement",
            "type: epic"
        ],
        "empty_content": "- no matching PRs"
    },
    {
        "title": "## 🐛 Bug Fixes",
        "labels": [
            "type: bug"
        ],
        "empty_content": "- no matching PRs"
    },
    {
        "title": "## 🧪 Maintenance",
        "labels": [
            "dependencies",
            "type: chore",
            "type: ci",
            "type: dependencies",
            "documentation"
        ],
        "empty_content": "- no matching changes"
    }
    ],
    "ignore_labels": [
        "duplicate",
        "question",
        "invalid",
        "wontfix"
    ],
    "template": "#{{CHANGELOG}}\n\n### 📦 Uncategorized\n\n${{UNCATEGORIZED}}",
    "pr_template": "- #{{TITLE}}",
    "base_branches": [
        "dev"
    ]
}
  